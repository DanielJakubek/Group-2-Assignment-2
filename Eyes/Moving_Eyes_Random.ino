#include <Servo.h>Servo eye1;  // create servo object to control an eyeServo eye2;  // create servo object to control an eyeint value;    // variable to read the value from the analog pinint delay_time;void setup() {  eye1.attach(9);  // attaches the eye on pin 9 to the servo object  eye2.attach(10);  // attaches the eye on pin 11 to the servo object  pinMode(13, OUTPUT);}void loop() {    int rand = random(70, 180);     delay_time = map(rand, 70, 180, 400, 5000);     // Eye 1    eye1.write(rand);  // sets the eye position according to the scaled value    delay(delay_time);               // Delay for a random time    //Eye 2     eye2.write(rand);                  // sets the servo position according to the scaled value   delay(delay_time);               // Delay for a random time}